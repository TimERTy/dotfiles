export ANDROID_HOME="/home/wm-admin/Android/Sdk/"
export PATH=$PATH:/home/wm-admin/Android/Sdk/platform-tools
export PATH=$PATH:/home/wm-admin/Android/Sdk/tools

alias setJava8="sudo update-alternatives --config java <<< $'2' && sudo update-alternatives --config javac <<< $'2'"
