export ANDROID_HOME="/home/wm-admin/Android/Sdk/"
export PATH=$PATH:/home/wm-admin/Android/Sdk/platform-tools
export PATH=$PATH:/home/wm-admin/Android/Sdk/tools
export PATH="$PATH:/home/timerty/.deno/bin"

alias setJava8="sudo update-alternatives --config java <<< $'2' && sudo update-alternatives --config javac <<< $'2'"
alias setJava11="sudo update-alternatives --config java <<< $'0' && sudo update-alternatives --config javac <<< $'0'"

alias setPhp56="sudo update-alternatives --config php <<< $'1' && a2dismod php7.2 && a2enmod php5.6 && service apache2 restart"
alias setPhp72="sudo update-alternatives --config php <<< $'0' && a2dismod php5.6 && a2enmod php7.2 && service apache2 restart"
