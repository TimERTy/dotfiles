alias ll='ls -lF'
alias la='ls -Al'
alias l='ls -F1'
alias alert='notify-send --urgency=low -i "$([ $? = 0 ] && echo terminal || echo error)" "$(history|tail -n1|sed -e '\''s/^\s*[0-9]\+\s*//;s/[;&|]\s*alert$//'\'')"'
alias setJava8="sudo update-alternatives --config java <<< $'2' && sudo update-alternatives --config javac <<< $'2'"
alias setJava11="sudo update-alternatives --config java <<< $'0' && sudo update-alternatives --config javac <<< $'0'"
alias setJavaHome="JAVA_HOME=$(dirname $(dirname $(readlink -f $(which javac)))); $(echo $JAVA_HOME)"

ANDROID_HOME=/home/wm-admin/Android/Sdk/platforms/android-27
